public class Car
{
	private int xPos, yPos, width, height;
	public Car(int x, int y, int w, int h)
	{
		setxPos(x);
		setyPos(y);
		setWidth(w);
		setHeight(h);
	}
	public int getHeight()
	{
		return height;
	}
	public void setHeight(int height)
	{
		this.height = height;
	}
	public int getWidth()
	{
		return width;
	}
	public void setWidth(int width)
	{
		this.width = width;
	}
	public int getxPos()
	{
		return xPos;
	}
	public void setxPos(int xPos)
	{
		this.xPos = xPos;
	}
	public int getyPos()
	{
		return yPos;
	}
	public void setyPos(int yPos)
	{
		this.yPos = yPos;
	}
	
}
